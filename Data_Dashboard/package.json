{
  "name": "DataDash",
  "version": "0.80.5",
  "license": "Apache-2.0",
  "main": "./build/electron/main.js",
  "scripts": {
    "dump": "dump-stlite-desktop-artifacts",
    "serve": "cross-env NODE_ENV=production electron .",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder"
  },
  "build": {
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "@stlite/desktop": "^0.80.5",
    "cross-env": "^7.0.3",
    "electron": "34.3.0",
    "electron-builder": "^25.1.7"
  },
  "stlite": {
    "desktop": {
      "nodeJsWorker": true,
      "nodefsMountpoints": {
        "/files": "{{desktop}}" 
      },
      "files": [
        "Data_Trends.py",
        "pages/*.py",
        ".streamlit/config.toml",
        "MedACRModules/*.py",
        "Test.py",
        "MedACR_ToleranceTableCheckerV2.py",
        "GetSNRData.py",
        "hazenlib/*.py",
        "hazenlib/tasks/*.py"
      ],
      "entrypoint": "Data_Trends.py",
      "requirementsTxtFiles": [
        "requirements.txt"
      ]
    }
  }
}